



<!---displays a list of managers in a table format. It also includes placeholders for adding, 
editing, and deleting manager records-->

<!-- managers-list-template.html -->


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ManagerRocords}}</title>
</head>

<body>

<table id="managersTable">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">firstname</th>
      <th scope="col">lastname</th>
      <th scope="col">Department</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each managers}}
      <tr data-manager={{id}}="">
        <td>{{this.id}}</td>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.position}}</td>
        <td>
          <button class="editManager">Edit</button>
          <button class="addManager">Add</button>
          <button class="deleteManager">Delete</button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
</body>



<script src="/js/managers.js"></script>